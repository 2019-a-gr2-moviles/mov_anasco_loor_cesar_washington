<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_main"
        tools:context=".MainActivity">

	<Button
			android:text="MENU ENTRENADOR"
			android:layout_width="180dp"
			android:layout_height="49dp"
			android:id="@+id/botonEntrenador"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="115dp"
			android:layout_marginStart="115dp"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="116dp"
			android:layout_marginRight="116dp"
			android:layout_marginBottom="354dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:layout_marginTop="18dp"
			app:layout_constraintTop_toBottomOf="@+id/botonPokemon"/>

	<Button
			android:text="MENU POKEMON"
			android:layout_width="179dp"
			android:layout_height="49dp"
			android:id="@+id/botonPokemon"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="116dp"
			android:layout_marginStart="116dp"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="116dp"
			android:layout_marginRight="116dp"
			android:layout_marginTop="205dp"
			app:layout_constraintTop_toTopOf="parent"
			android:layout_marginBottom="18dp"
			app:layout_constraintBottom_toTopOf="@+id/botonEntrenador"/>
</android.support.constraint.ConstraintLayout>